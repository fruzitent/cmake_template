{
  "format": {
    "dangle_parens": false,
    "line_ending": "unix",
    "line_width": 120,
    "max_pargs_hwrap": 3,
    "separate_ctrl_name_with_space": false,
    "separate_fn_name_with_space": false,
    "tab_size": 2
  },
  "markup": {
    "bullet_char": "*",
    "enable_markup": false,
    "enum_char": "."
  },
  "parse": {
    "additional_commands": {
      "foo": {
        "flags": ["BAR", "BAZ"],
        "kwargs": {
          "DEPENDS": "*",
          "HEADERS": "*",
          "SOURCES": "*"
        }
      }
    }
  }
}
